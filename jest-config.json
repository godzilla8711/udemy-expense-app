{
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "testRegex": "\\-test\\.js$",
  "collectCoverage": true,
  "coverageDirectory": "reports",
  "collectCoverageFrom": [
    "src/**",
    "!src/playground/**"
  ],
  "setupFiles": [
    "raf/polyfill",
    "<rootDir>/test/setupTest.js"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ]
}
